@model MVC9pmTigersBatch.Models.EmpDept
@{
    ViewBag.Title = "SendMultipleinfoByModel2";
}

<h2>SendMultipleinfoByModel2</h2>


<table>
    <tr>
        <th>EmpId</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in Model.emp)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table><br />

<table>
    <tr>
        <th>DeptId</th>
        <th>Name</th>
         
    </tr>
    @foreach (var item in Model.dept)
    {
        <tr>
            <td>@item.DeptId</td>
            <td>@item.DeptName</td>
        </tr>
    }
</table>