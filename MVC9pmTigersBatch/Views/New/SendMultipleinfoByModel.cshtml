@model List<MVC9pmTigersBatch.Models.EmployeeModel>
@{
    ViewBag.Title = "SendMultipleinfoByModel";
}

<h2>Send Multiple info By Model</h2>


<table class="table table-striped table-condensed">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }

</table>
